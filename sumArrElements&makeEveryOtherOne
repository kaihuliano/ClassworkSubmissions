public static void main(String[] args) {
		// TODO Auto-generated method stub

		//instantiate double that will hold the sum
		double sum;
		
		//create a new array of double
		double[] baka = {7.6, 9.4, 9.2, 2.2};
		
		//call a method with the doule array passed to it
		sumArr(baka);
		
		//print out what the method returns
		System.out.println(sumArr(baka));
		
		//create a new int array
		int [] nums = { 3, 17, 12, 8, 9, 4, 12 };
		
		//make a new int array equal to a method that traverses the array and returns new
		int [] brr = MakeEveryOther( nums );
		
		
	}
		/**
		 * a method makeEveryOtherOne that is passed an array of ints and changes that array. 
		 * It should make every other element in the array '1' starting with the second element.  
		 * @param nums
		 * @return
		 */
		private static int[] MakeEveryOther(int[] nums) {
			// TODO Auto-generated method stub
			
			//for loop that traverses every other element in an array
			for ( int i = 1; i <= nums.length - 1; i+= 2)
			{
				//change every other number at index i to 1
				nums[i] = 1;
			}
			//return the new element value
			return nums;
		}
		
	
		

	/**
	 * method that will return sum of all the elements in array
	 * @param baka
	 * @return 
	 */
	public static double sumArr(double[] baka) {
		// TODO Auto-generated method stub
		
		//create a placeholder for the value
		double sum = 0;
		
		//for loop that traverses all elements in the array
		for(int i = 0; i < baka.length - 1; i++)
		{
			//add each element to get the sum
			sum += baka[i];
		}
		//return the sum of all elements in the array
		return sum;
	

	}
	
	

}
